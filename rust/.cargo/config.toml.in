# config.toml.in Template file used by build.rs

[target.aarch64-linux-android]
linker = "@ANDROID_SDK_HOME@/ndk/latest/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android26-clang"

[target.armv7-linux-androideabi]
linker = "@ANDROID_SDK_HOME@/ndk/latest/toolchains/llvm/prebuilt/linux-x86_64/bin/armv7a-linux-androideabi26-clang"

[target.i686-linux-android]
linker = "@ANDROID_SDK_HOME@/ndk/latest/toolchains/llvm/prebuilt/linux-x86_64/bin/i686-linux-android26-clang"

[target.x86_64-linux-android]
linker = "@ANDROID_SDK_HOME@/ndk/latest/toolchains/llvm/prebuilt/linux-x86_64/bin/x86_64-linux-android26-clang"

# Set default target to arm64 for rust-analyzer
[build]
target = "aarch64-linux-android"
